{
  "name": "centsa",
  "version": "2.0.5",
  "description": "Centsa money managing solution",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "axios": "0.17.1",
    "cookie-parser": "1.4.3",
    "express": "4.16.2",
    "express-fileupload": "0.4.0",
    "express-session": "1.15.6",
    "express-unless": "0.5.0",
    "mysql": "2.15.0",
    "session-file-store": "1.2.0"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/chai": "4.1.2",
    "@types/express": "4.11.0",
    "@types/express-session": "1.15.6",
    "@types/mocha": "2.2.48",
    "@types/mysql": "2.15.0",
    "@types/sinon": "4.3.0",
    "chai": "4.1.2",
    "istanbul": "0.4.5",
    "mocha": "5.0.4",
    "nyc": "11.4.1",
    "sinon": "4.1.5",
    "ts-lint": "4.5.1",
    "ts-node": "4.1.0",
    "ts-node-dev": "1.0.0-pre.9",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc -p .",
    "dev": "ts-node-dev src/index.ts",
    "debug": "NODE_ENV=debug ts-node --inspect-brk=9009 src/index.ts",
    "test": "nyc mocha -r ts-node/register test/**/*Test.ts",
    "prebuild": "npm test",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nohorjo/Centsa.git"
  },
  "author": "Muhammed Haque",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nohorjo/Centsa/issues"
  },
  "homepage": "https://github.com/nohorjo/Centsa#readme",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/*.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
